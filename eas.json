{
  "build": {
    "development": {
      "channel": "development",
      "ios": {
        "cocoapods": "1.14.3",
        "scheme": "Hodos Dev"
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_APP_VARIANT": "development",
        "EXPO_PUBLIC_APP_NAME": "Hodos Dev",
        "EXPO_PUBLIC_BUNDLE_ID": "com.genny.hodos.dev",
        "EXPO_PUBLIC_API": "https://life-manager-api.genny.id.vn/",
        "VIETMAP_API_URL": "https://maps.vietmap.vn/api/"
      }
    },
    "production": {
      "channel": "production",
      "ios": {
        "cocoapods": "1.14.3",
        "scheme": "APP"
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_APP_VARIANT": "production",
        "EXPO_PUBLIC_APP_NAME": "Hodos",
        "EXPO_PUBLIC_BUNDLE_ID": "com.genny.hodos.pro",
        "EXPO_PUBLIC_API": "https://life-manager-api.genny.id.vn/",
        "VIETMAP_API_URL": "https://maps.vietmap.vn/api/"
      }
    }
  },

  "cli": {
    "version": ">= 0.53.1",
    "promptToConfigurePushNotifications": false
  }
}
